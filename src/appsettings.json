{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Search": {
    "Index": "forward",
    "Score": "bm25",
    "ShowResultsOf": "Dictionary",
    "Tokenizer":{
      "EnableStemmer": true,
      "EnableStopWords": true
    }
  }
}
